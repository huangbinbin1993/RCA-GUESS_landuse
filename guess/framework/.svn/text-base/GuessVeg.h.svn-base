class GuessVegArchive {

private:

	FILE* pfile;
	long recno;
	long datano;
	unsigned char pindex[GUESSVEG_INDEX_LENGTH];
	unsigned char pdata[GUESSVEG_DATA_LENGTH];
	bool iseof;

	long readbin(int nbyte) {

		unsigned char buf[4];
		long mult[4]={0x1000000,0x10000,0x100,1},val;
		int i;

		fread(buf,nbyte,1,pfile);

		val=0;
		for (i=0;i<nbyte;i++) {
			val+=buf[i]*mult[4-nbyte+i];
		}

		return val;
	}

	void getindex(long n) {

		fseek(pfile,GUESSVEG_INDEX_LENGTH*(n-recno),SEEK_CUR);
		fread(pindex,GUESSVEG_INDEX_LENGTH,1,pfile);
		datano=pindex[GUESSVEG_INDEX_LENGTH-4]*0x1000000+pindex[GUESSVEG_INDEX_LENGTH-3]*0x10000+
			pindex[GUESSVEG_INDEX_LENGTH-2]*0x100+pindex[GUESSVEG_INDEX_LENGTH-1];
		recno=n+1;
		iseof=(recno==GUESSVEG_NRECORD);
	}

	void getdata() {

		fseek(pfile,GUESSVEG_INDEX_LENGTH*-recno+(datano-GUESSVEG_NRECORD)*GUESSVEG_DATA_LENGTH,SEEK_CUR);
		fread(pdata,GUESSVEG_DATA_LENGTH,1,pfile);
		fseek(pfile,GUESSVEG_DATA_LENGTH*(GUESSVEG_NRECORD-datano-1)+GUESSVEG_INDEX_LENGTH*recno,SEEK_CUR);
	}

	double popreal(unsigned char* bits,int nbyte,int nbit,double scalar,double offset) {

		unsigned char buf;
		int nb=nbit/8,i;
		double rval=0.0;
		long mult[4]={1,0x100,0x10000,0x1000000};

		for (i=0;i<4;i++) {
			if (i<nb) rval+=bits[nbyte-i-1]*mult[i];
			else if (i==nb) {
				buf=bits[nbyte-i-1]<<(8-nbit%8);
				buf>>=8-nbit%8;
				rval+=buf*mult[i];
			}
		}

		for (i=nbyte-1;i>=0;i--) {
			if (i>=nb)
				bits[i]=bits[i-nb];
			else
				bits[i]=0;
		}

		nb=nbit%8;

		for (i=nbyte-1;i>=0;i--) {
			bits[i]>>=nb;
			if (i>0) {
				buf=bits[i-1];
				buf<<=8-nb;
				bits[i]|=buf;
			}
		}

		rval=rval*scalar+offset;

		return rval;
	}

	void bitify(unsigned char buf[4],double fval,double offset,double scalar) {

		long ival = (long)((fval-offset)/scalar + 0.5);
		buf[0]=(unsigned char)(ival/0x1000000);
		ival-=buf[0]*0x1000000;
		buf[1]=(unsigned char)(ival/0x10000);
		ival-=buf[1]*0x10000;
		buf[2]=(unsigned char)(ival/0x100);
		ival-=buf[2]*0x100;
		buf[3]=(unsigned char)(ival);
	}

	void merge(unsigned char ptarget[3],unsigned char buf[4],int bits) {

		int nb=bits/8;
		int i;
		unsigned char nib;
		for (i=0;i<3;i++) {

			if (i<3-nb)
				ptarget[i]=ptarget[i+nb];
			else
				ptarget[i]=0;
		}
		nb=bits%8;
		for (i=0;i<3;i++) {
			ptarget[i]<<=nb;
			if (i<3-1) {
				nib=ptarget[i+1]>>(8-nb);
				ptarget[i]|=nib;
			}
		}

		nb=bits/8;
		if (bits%8) nb++;
		for (i=1;i<=nb;i++)
			ptarget[3-i]|=buf[4-i];
	}

	int compare_index(unsigned char* a,unsigned char* b) {

		int i;
		for (i=0;i<3;i++) {
			if (a[i]<b[i]) return -1;
			else if (a[i]>b[i]) return +1;
		}

		return 0;
	}

	bool initialise(const char* filename) {

		int i;
		unsigned char* pheader;

		if (pfile) fclose(pfile);
		pfile=fopen(filename,"rb");
		if (!pfile) {
			printf("Could not open %s for input\n",filename);
			return false;
		}

		pheader=new unsigned char[GUESSVEG_HEADERSIZE-4];
		if (!pheader) {
			printf("Out of memory\n");
			fclose(pfile);
			pfile=NULL;
			return false;
		}
		::rewind(pfile);
		fread(pheader,GUESSVEG_HEADERSIZE-4,1,pfile);
		for (i=0;i<GUESSVEG_HEADERSIZE-4;i++) {
			if (pheader[i]!=GUESSVEG_HEADER[i]) {
				printf("Format of %s incompatible with this version of GuessVeg.h\n",filename);
				fclose(pfile);
				pfile=NULL;
				delete pheader;
				return false;
			}
		}
		delete[] pheader;

		::rewind(pfile);
		fseek(pfile,GUESSVEG_HEADERSIZE+GUESSVEG_DATA_LENGTH*GUESSVEG_NRECORD,SEEK_CUR);
		recno=0;
		iseof=false;

		return true;
	}

public:

	GuessVegArchive() {
		pfile=NULL;
	}

	~GuessVegArchive() {
		if (pfile) fclose(pfile);
	}

	bool open(const char* filename) {
		return initialise(filename);
	}

	bool open() {
		return open("GuessVeg.bin");
	}

	void close() {
		if (pfile) {
			fclose(pfile);
			pfile=NULL;
		}
	}

	bool rewind() {

		if (!pfile) return false;

		::rewind(pfile);
		fseek(pfile,GUESSVEG_HEADERSIZE+GUESSVEG_DATA_LENGTH*GUESSVEG_NRECORD,SEEK_CUR);
		recno=0;
		iseof=false;

		return true;
	}

	bool getnext(GuessVeg& obj) {

		if (!pfile || iseof) return false;

		int i;

		getindex(recno);
		getdata();

		obj.gridcell_id=popreal(pindex,3,11,1,0);
		obj.process_id=popreal(pindex,3,6,1,0);

		for (i=0;i>=0;i--) obj.laimax_broadleaf[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
		for (i=0;i>=0;i--) obj.laimax_conifer[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
		for (i=365;i>=0;i--) obj.laiphen_broadleaf[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
		for (i=365;i>=0;i--) obj.laiphen_conifer[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
		for (i=365;i>=0;i--) obj.laiphen_grass_for[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
		for (i=365;i>=0;i--) obj.laiphen_grass_opl[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);

		return true;
	}

	bool getindex(GuessVeg& obj) {

		if (!GUESSVEG_NRECORD || !pfile) return false;

		// else

		unsigned char ptarget[3]={0,0,0};
		unsigned char buf[4];
		bitify(buf,obj.process_id,0,1);
		merge(ptarget,buf,6);
		bitify(buf,obj.gridcell_id,0,1);
		merge(ptarget,buf,11);

		long begin = 0;
		long end = GUESSVEG_NRECORD;

		while (begin < end) {
			long middle = (begin+end)/2;

			getindex(middle);
			getdata();

			int c = compare_index(pindex, ptarget);

			if (c < 0) {
				begin = middle + 1;
			}
			else if (c > 0) {
				end = middle;
			}
			else {

				for (int i=0;i>=0;i--) obj.laimax_broadleaf[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
				for (int i=0;i>=0;i--) obj.laimax_conifer[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
				for (int i=365;i>=0;i--) obj.laiphen_broadleaf[i]=popreal(pdata,GUESSVEG_DATA_LENGTH,11,0.01,0);
